{
    "builds": [
        {
            "src": "api/server.py",
            "use": "@vercel/python"
        },
        {
            "src": "web",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/server.py"
        },
        {
            "src": "/(.*)",
            "dest": "web/index.html"
        }
    ],
    "includeFiles": [
        "api/MusicManager.py"
      ]
}