{
    "builds": [
        {
            "src": "api/server.py",
            "use": "@vercel/python"
        },
        {
            "src": "/index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/server.py"
        },
        {
            "src": ".*",
            "dest": "index.html"
        }
    ]
}